<!-- MAIN CONTENT -->
<main class="materi-content mt-2 mb-5 px-lg-5 pb-2">
    <section class="container-sm px-lg-5">
        <div class="container">
            <div class="row">
                <div class="col-md-2">
                </div>
                <div class="col-md-8">
                    <article>
                        <h2><%- judul %></h2>
                        <% isi.forEach(el => { %>
                        <p><%- el %> </p>
                        <% }) %>
                    </article>
                    <article>
                        <p>Anda juga bisa menonton vidio untuk tambahan informasi</p>
                        <div class="ratio ratio-4x3">
                            <iframe width="560" height="315" src="<%- linkVidio %>"
                                title="YouTube video player" frameborder="0"
                                allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                allowfullscreen></iframe>
                        </div>
                    </article>
                </div>
                <div class="col-md-2">
                </div>
            </div>
        </div>
    </section>
</main>
<!-- END MAIN CONTENT-->

<!-- FOOTER -->
<footer class="w-100 bg-dark text-center text-capitalize text-bg-dark pt-2 fixed-bottom">
    <p><%- judul %></p>
</footer>
<!-- END FOOTER -->